server {
    listen 80;

    server_name mtc-home-kitchen.local;
    #sockets
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "upgrade";

    location / {
        proxy_pass http://client:3000;
    }

    location /api {
        proxy_pass http://server:80;
    }
}
